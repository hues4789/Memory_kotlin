<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.memory.memory_kotlin">

    <application
        android:name=".BootStrap"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TaskCreateLoginActivity"></activity>
        <activity android:name=".TodayToDoTaskLoginActivity"></activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".TodayToDoTaskActivity" />
        <activity android:name=".TaskCreateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>